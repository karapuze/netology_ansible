# This is a standard Vector configuration file.
# See https://vector.dev/docs/setup/guides/getting-started for more info.

[sources.in]
  type = "stdin"

[transforms.json_parser]
  type = "json_parser"
  inputs = ["in"]
  drop_invalid = true

[sinks.out]
  type = "console"
  inputs = ["json_parser"]
  encoding.codec = "json"
